<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/btnImieNazwisko"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="WYWOŁAJ AKTYWNOŚĆ Z IMIENIEM I NAZWISKIEM"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <Button
        android:id="@+id/btnAdres"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="WYWOŁAJ APLIKACJĘ OBSŁUGUJĄCĄ PODANY ADRES URL"
        app:layout_constraintTop_toBottomOf="@id/btnImieNazwisko"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <Button
        android:id="@+id/btnGeo"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="WYŚWIETL AKTYWNOŚĆ Z DANYM POŁOŻENIEM (GEO)"
        app:layout_constraintTop_toBottomOf="@id/btnAdres"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <TextView
        android:id="@+id/tvURL"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="http://"
        android:textSize="20sp"
        app:layout_constraintTop_toBottomOf="@id/btnGeo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>
